		"svelte-eslint-parser": "^0.32.2",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-sqlite": "^2.0.0",
		"@lucia-auth/oauth": "^2.1.2",
		"better-sqlite3": "^8.5.1",
		"drizzle-orm": "^0.28.2",
		"lucia": "^2.3.0"
	}
